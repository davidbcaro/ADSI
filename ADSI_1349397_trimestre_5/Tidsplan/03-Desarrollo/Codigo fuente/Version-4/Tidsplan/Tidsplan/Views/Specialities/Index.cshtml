@model IEnumerable<Tidsplan.Models.Speciality>

@{
    ViewBag.Title = "Especialidad";
}

<h2>Especialidad</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.toUpper)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.toUpper)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.idSpeciality }, new { @class = "btn btn-warning" }) 
            @Html.ActionLink("Detalles", "Details", new { id = item.idSpeciality }, new { @class = "btn btn-info" })  
            @Html.ActionLink("Borrar", "Delete", new { id=item.idSpeciality }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
