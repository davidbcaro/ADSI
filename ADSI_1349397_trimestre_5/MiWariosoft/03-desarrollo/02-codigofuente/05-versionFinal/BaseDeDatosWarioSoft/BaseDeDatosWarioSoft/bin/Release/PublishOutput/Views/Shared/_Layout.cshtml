<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Wariosoft</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="icon"href="~/Imagenes/Wario.ico">

</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="collapse navbar-collapse" id="myNavbar">
                <ul class="nav navbar-nav navbar-right">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    @Html.Partial("_LoginPartial")

                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Admin
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Users", "Index", "Users")</li>
                            <li>@Html.ActionLink("TypeDocument", "Index", "TypeDocuments")</li>
                            <li>@Html.ActionLink("Linkages", "Index", "Linkages")</li>
                            <li>@Html.ActionLink("Specialties", "Index", "Specialties")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Shedule
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Shedules", "Index", "Shedules")</li>
                            <li>@Html.ActionLink("Headquarters", "Index", "Headquarters")</li>
                            <li>@Html.ActionLink("Environments", "Index", "Environments")</li>
                            <li>@Html.ActionLink("Modalities", "Index", "Modalities")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Group
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Groups", "Index", "Groups")</li>
                            <li>@Html.ActionLink("Trimesters", "Index", "Trimesters")</li>
                            <li>@Html.ActionLink("Projects", "Index", "Projects")</li>
                            <li>@Html.ActionLink("Phases", "Index", "Phases")</li>
                            <li>@Html.ActionLink("WorkingDays", "Index", "WorkingDays")</li>
                            <li>@Html.ActionLink("Versions", "Index", "Versions")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Formation
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Competitions", "Index", "Competitions")</li>
                            <li>@Html.ActionLink("Programs", "Index", "Programs")</li>
                            <li>@Html.ActionLink("TrainingLevels", "Index", "TrainingLevels")</li>
                            <li>@Html.ActionLink("LearningOutcomes", "Index", "LearningOutcomes")</li>
                            <li>@Html.ActionLink("Activities", "Index", "Activities")</li>
                        </ul>
                    </li>
                </ul>

            </div>
        </div>
    </nav>


    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Wariosoft</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
